{
  "name": "cypress-to-playwright",
  "version": "0.0.1",
  "description": "Automatic migration from cypress to playwright",
  "main": "index.js",
  "scripts": {
    "postinstall": "git config --local core.hooksPath .githooks/",
    "build": "tsc",
