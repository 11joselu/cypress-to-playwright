{
  "name": "@11joselu/cypress-to-playwright",
  "version": "0.0.1",
  "description": "Automatic migration from cypress to playwright",
  "main": "dist/cjs/converter.js",
  "module": "dist/esm/converter.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "dist/esm/index.d.ts",
        "default": "dist/esm/converter.mjs"
      },
      "require": {
        "types": "dist/cjs/index.d.ts",
        "default": "dist/cjs/converter.js"
      },
      "default": "dist/cjs/converter.js"
    }
  },
  "files": [
    "dist/"
  ],
  "bin": {
    "cypress-to-playwright": "./bin/index.js"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "postinstall": "git config --local core.hooksPath .githooks/",
    "lint": "eslint **/*.ts --fix",
    "lint-staged": "lint-staged",
    "test": "tsc --noEmit && node --loader tsx --no-warnings --test-reporter=spec --test test/*.test.ts",
    "binary": "tsc --noEmit && node --loader tsx src/bin/index.ts",
    "build": "tsc -p ./tsconfig.json && tsc -p ./tsconfig.cjs.json && tsconfig-to-dual-package",
    "postbuild": "find dist/esm -type f -name \"*.js\" -exec sh -c 'mv \"$0\" \"${0%.js}.mjs\"' {} \\;\n",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/11joselu/cypress-to-playwright.git"
  },
  "keywords": [
    "Cypress",
    "Playwright",
    "e2e",
    "test"
  ],
  "author": "Jose Cabrera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11joselu/cypress-to-playwright/issues"
  },
  "homepage": "https://github.com/11joselu/cypress-to-playwright#readme",
  "devDependencies": {
    "@types/node": "18.14.2",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "tsconfig-to-dual-package": "1.1.1",
    "tsx": "3.12.3"
  },
  "dependencies": {
    "typescript": "4.9.5"
  }
}
